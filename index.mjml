                </mj-social-element>
                <mj-social-element padding="10px"  src="img/twitter.png" href="#"> 
                </mj-social-element>
                <mj-social-element padding="10px"  src="img/instagram.png" href="#"> 
                </mj-social-element>
                <mj-social-element padding="10px"  src="img/linkedin.png" href="#"> 
                </mj-social-element>
